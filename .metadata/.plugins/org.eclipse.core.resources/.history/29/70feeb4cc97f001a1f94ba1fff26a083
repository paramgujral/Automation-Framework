package all.automation;

import org.openqa.selenium.By;

public class ActionEngine extends TestEngine{
	
	/*
	
	example -- Click
	*/
	public static void click(By locator, String locatorName)
			throws Throwable {
		//Wait
		try {
			driver.findElement(locator).click();
		} catch (Exception e) {
			e.printStackTrace();
			}	
	}
	 
	/* 
	Tab --
	Longpress --
	DoubleTab --
	ScrollToElement --
	Swipe --
	sendKey --
	isElementDisplay --
	WaitforElement --
	draganddrop --
	switchWindowByTitle --
	clikcOnAlert ---

	*/

}
